
@{
    ViewBag.Title = "Dividir";
}

@using CalculadoraPrecaria.Models
@model Calculadora

<html>
<head>

</head>
<body>
    <h2>Calcular</h2>
    <div class="text-center">
        <form action="/Calculadora/Dividir" method="Post">

            <div class="form-group">
                <label>Numero: @Html.TextBoxFor(c => c.primerNumero, null, new { @class = "form-control" }) </label>
            </div>
            <div class="form-group">
                <label>Numero: @Html.TextBoxFor(c => c.segundoNumero, null, new { @class = "form-control" }) </label>
            </div>


            <input class="btn" type="submit" value="Calcular" />


        </form>
    </div>


</body>

</html>

